	Number = {35},
	Pages = {4404 - 4411},
	Title = {Higher vaccine effectiveness in seasons with predominant circulation of seasonal influenza A(H1N1) than in A(H3N2) seasons: Test-negative case-control studies using surveillance data, Spain, 2003-2011},
