	Journal = {Epidemiology (Cambridge, Mass.)},
	Keywords = {Disease Progression; HIV Infections/epidemiology/*transmission; Homosexuality, Male; Humans; Male; Models, Statistical; Risk Factors; *Sexual Behavior; *Sexual Partners; Time Factors},
