	Doi = {https://doi.org/10.1016/j.vaccine.2019.06.019},
	Issn = {0264-410X},
	Journal = {Vaccine},
	Keywords = {Vaccine trial design, Outbreaks, Epidemics, Vaccines, Public Health Emergency, Emerging infectious diseases, Scientific communication, Phase III trial, Decision support system},
	Title = {An online decision tree for vaccine efficacy trial design during infectious disease epidemics: The InterVax-Tool},
	Url = {http://www.sciencedirect.com/science/article/pii/S0264410X19307807},
	Year = {2019},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0264410X19307807},
