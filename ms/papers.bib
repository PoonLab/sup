	Doi = {10.1098/rspb.2018.2201},
	Journal = {Proceedings of the Royal Society B: Biological Sciences},
	Number = {1893},
	Pages = {20182201},
	Title = {Household members do not contact each other at random: implications for infectious disease modelling},
	Volume = {285},
	Year = {2018},
