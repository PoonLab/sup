	Pages = {1-15},
	Publisher = {Public Library of Science},
	Title = {Low 2012--13 Influenza Vaccine Effectiveness Associated with Mutation in the Egg-Adapted H3N2 Vaccine Strain Not Antigenic Drift in Circulating Viruses},
