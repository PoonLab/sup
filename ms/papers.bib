	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Identification of hidden population structure in time-scaled phylogenies},
	Url = {https://www.biorxiv.org/content/early/2019/07/16/704528},
	Year = {2019},
	Bdsk-File-1 = {},
