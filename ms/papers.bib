	Issn = {0264-410X},
	Journal = {Vaccine},
	Keywords = {Emergency response, Priority pathogens, Development pipeline, Vaccine development tool},
	Title = {Process mapping of vaccines: Understanding the limitations in current response to emerging epidemic threats},
	Url = {http://www.sciencedirect.com/science/article/pii/S0264410X19301264},
	Year = {2019},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0264410X19301264},
