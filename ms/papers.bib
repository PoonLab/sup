	Issn = {1755-4365},
	Journal = {Epidemics},
	Keywords = {Infectious disease modeling, Generation interval, Basic reproduction number},
	Pages = {12 - 18},
	Title = {A practical generation-interval-based approach to inferring the strength of epidemics from their speed},
