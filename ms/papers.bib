	Keywords = {Contact Tracing, Coronavirus Epidemics, Transmission Modelling, SARS, MERS},
	Title = {Epidemic Models of Contact Tracing: Systematic Review of Transmission Studies of Severe Acute Respiratory Syndrome and Middle East Respiratory Syndrome},
