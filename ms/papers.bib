	Journal = {Epidemics},
	Journal-Full = {Epidemics},
	Keywords = {Concurrent partnerships; Pair-formation model; Sexual networks; Vaccination},
	Mesh = {Adult; Female; Humans; Male; Models, Theoretical; Prevalence; Sexual Behavior; Sexual Partners; Sexually Transmitted Diseases; Vaccination},
	Month = {12},
	Pages = {35-46},
	Pmid = {29798812},
