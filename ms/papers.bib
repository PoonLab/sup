	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Seasonal influenza: Modelling approaches to capture immunity propagation},
	Url = {https://www.biorxiv.org/content/early/2019/05/13/637074},
