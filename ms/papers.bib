	Issn = {0196-6553},
	Journal = {American Journal of Infection Control},
	Keywords = {Nosocomial diarrhea, Infection control, Hospital epidemiology},
	Title = {Clostridium difficile intervention timelines for diagnosis, isolation, and treatment},
	Url = {http://www.sciencedirect.com/science/article/pii/S0196655319305218},
	Year = {2019},
