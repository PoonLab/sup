	Doi = {10.1006/jtbi.2001.2347},
	Journal = {J Theor Biol},
	Journal-Full = {Journal of theoretical biology},
	Mesh = {Algorithms; Antibodies, Viral; Antibody Affinity; Antigen-Antibody Reactions; Antigens, Viral; Hemagglutination Inhibition Tests; Humans; Influenza, Human; Models, Immunological; Orthomyxoviridae},
	Month = {Sep},
