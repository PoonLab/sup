@article{Dombrowski:2018,
	Abstract = {{(See the Breif Report by Xia et al on pages 851--5.)Viral suppression in persons living with human immunodeficiency virus (HIV) saves lives and prevents new infections [1, 2]. Recently, the ``undetectable=untransmittable'' (U=U) concept has been embraced by community advocates and widely endorsed by scientific and public health agencies as a stigma-combating message grounded in rigorous science [3]. As the science behind U=U has fallen into place, so too has the drive to ensure that persons in which HIV infection has been newly diagnosed achieve viral suppression quickly. However, neither of the 2 flagship strategies guiding HIV prevention and care in the United States (the Joint United Nations Program on AIDS [UNAIDS] 90-90-90 targets [4] and the US National HIV/AIDS Strategy [NHAS] [5]) incorporates a time element. The numeric goals of UNAIDS and the NHAS---81\\% and 80\\% viral suppression, respectively, among persons with a diagnosis of HIV infection---measure only a cross-sectional snapshot of the population. Yet a person who reaches viral suppression within 1 month of diagnosis reflects a greater individual and public health success than one who reaches viral suppression 1 year after diagnosis.}},
	Author = {Dombrowski, Julia C and Baeten, Jared M},
