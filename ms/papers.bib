	Journal = {Infect Control Hosp Epidemiol},
	Journal-Full = {Infection control and hospital epidemiology},
	Mesh = {Aged; Aged, 80 and over; Bacterial Typing Techniques; Clostridium Infections; Clostridium difficile; Enterocolitis, Pseudomembranous; Environmental Microbiology; Feces; Female; Hospital Units; Humans; Male; Metronidazole; Middle Aged; Ribotyping; Skin; Spores, Bacterial; Treatment Outcome; Vancomycin},
	Month = {Jan},
	Number = {1},
	Pages = {21-7},
	Pmid = {19929371},
	Pst = {ppublish},
	Title = {Persistence of skin contamination and environmental shedding of Clostridium difficile during and after treatment of C. difficile infection},
