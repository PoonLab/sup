	Pages = {1-9},
	Publisher = {Public Library of Science},
	Title = {Influenza Human Monoclonal Antibody 1F1 Interacts with Three Major Antigenic Sites and Residues Mediating Human Receptor Specificity in H1N1 Viruses},
