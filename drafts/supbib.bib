@article{ehichioyaPhylogeographyLassaVirus2019,
  title = {Phylogeography of {{Lassa Virus}} in {{Nigeria}}},
  author = {Ehichioya, Deborah U. and Dellicour, Simon and Pahlmann, Meike and Rieger, Toni and Oestereich, Lisa and {Becker-Ziaja}, Beate and Cadar, Daniel and Ighodalo, Yemisi and Olokor, Thomas and Omomoh, Emmanuel and Oyakhilome, Jennifer and Omiunu, Racheal and Agbukor, Jacqueline and Ebo, Benevolence and Aiyepada, John and Ebhodaghe, Paulson and Osiemi, Blessing and Ehikhametalor, Solomon and Akhilomen, Patience and Airende, Michael and Esumeh, Rita and Muoebonam, Ekene and Giwa, Rosemary and Ekanem, Anieno and Igenegbale, Ganiyu and Odigie, George and Okonofua, Grace and Enigbe, Racheal and Yerumoh, Edna Omonegho and Pallasch, Elisa and Bockholt, Sabrina and Kafetzopoulou, Liana E. and Duraffour, Sophie and Okokhere, Peter O. and Akpede, George O. and Okogbenin, Sylvanus A. and Odia, Ikponmwosa and Aire, Chris and Akpede, Nosa and Tobin, Ekaete and {Ogbaini-Emovon}, Ephraim and Lemey, Philippe and Adomeh, Donatus I. and Asogun, Danny A. and G{\"u}nther, Stephan},
