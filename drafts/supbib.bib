@article{gompertHierarchicalBayesianModel2011,
  title = {A {{Hierarchical Bayesian Model}} for {{Next}}-{{Generation Population Genomics}}},
  author = {Gompert, Zachariah and Buerkle, C. Alex},
  year = {2011},
  month = mar,
  journal = {Genetics},
  volume = {187},
