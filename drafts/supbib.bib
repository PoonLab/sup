@article{lemeyBayesianPhylogeographyFinds2009,
  title = {Bayesian {{Phylogeography Finds Its Roots}}},
  author = {Lemey, Philippe and Rambaut, Andrew and Drummond, Alexei J. and Suchard, Marc A.},
  year = {2009},
