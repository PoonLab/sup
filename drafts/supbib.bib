@article{suchardModelsEstimatingBayes2005,
  title = {Models for {{Estimating Bayes Factors}} with {{Applications}} to {{Phylogeny}} and {{Tests}} of {{Monophyly}}},
  author = {Suchard, Marc A. and Weiss, Robert E. and Sinsheimer, Janet S.},
  year = {2005},
  volume = {61},
